{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/contact_form",
        "function": "contactForm"
      },
      {
        "source":"**",
        "destination":"/index.html"
      }
    ]
  }
}